  <h1>Listing All Users</h1>
  <table>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Complete Detail</th>
      <th>Edit User</th>
    </tr>
  
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
      </tr>
    <% end %>
  </table>
  <br>
  <%= link_to 'Add New User', new_user_path %>
